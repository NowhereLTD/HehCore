{
  "name": "WebRouter",
  "type": "Router",
  "routes": {
    "/": {
      "name": "Home",
      "path": "Home.class.js",
      "method": "*"
    },
    "*": {
      "name": "AssetOrErrorPage",
      "path": "AssetOrErrorPage.class.js",
      "method": "*"
    },
    "/example.json": {
      "name": "JSONPage",
      "path": "JSONPage.class.js",
      "method": "GET"
    }
  },
  "settings": {
    "resourceDirectory": "assets"
  }
}
