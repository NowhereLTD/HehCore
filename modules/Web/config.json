{
  "name": "WebRouter",
  "type": "Router",
  "routes": {
    "/": {
      "name": "Home",
      "path": "Home.class.js",
      "method": "*"
    },
    "*": {
      "name": "ErrorPage",
      "path": "ErrorPage.class.js",
      "method": "*"
    },
    "/example.json": {
      "name": "JSONPage",
      "path": "JSONPage.class.js",
      "method": "GET"
    }
  }
}
